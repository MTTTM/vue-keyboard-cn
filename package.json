{
  "name": "vue-keyboard-cn",
  "description": "vue virtual keyboard for wap",
  "version": "0.0.2",
  "author": "MTTTM",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.es.min.js",
  "keywords": [
    "vue",
    "keyboard",
    "virtual",
    "keyboard-key",
    "virtual keyboard"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env BABEL_ENV=test nyc --reporter=lcov --reporter=text karma start",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "distDemo": "cross-env NODE_ENV=production webpack --config webpack.demo.js",
    "distLibs": "cross-env NODE_ENV=production webpack --config webpack.components.js",
    "dist": "cross-env NODE_ENV=production webpack --config webpack.libs.dist.js"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "core-js": "^3.13.1",
    "disable-body-scroll": "^0.1.3",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.14.1",
    "sass": "^1.34.0",
    "vue": "^2.5.11",
    "vue-horizontal-screen": "^0.1.15",
    "vue-router": "^3.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.9",
    "mocha": "^5.0.0",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^7.3.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "nyc": {
    "include": [
      "src/dev/**/*.js",
      "src/dev/**/*.vue"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
